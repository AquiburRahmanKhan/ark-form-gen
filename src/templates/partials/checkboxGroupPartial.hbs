<Form.Item 
  {{#if fieldProps.label}}
  label="{{ fieldProps.label }}"
  {{/if}}
  {{#if fieldProps.name}}
  name="{{ fieldProps.name }}"
  {{/if}}
  {{#if fieldProps.rules}}
  rules={[ 
    {{#each fieldProps.rules }}
    {{{this}}},
    {{/each}} 
  ]}
  {{/if}}
  >
  <Checkbox.Group>
    <Row>
      {{#each fieldProps.options }}
      <Col span={8}>
        <Checkbox value="{{this.value}}">{{this.label}}</Checkbox>    
      </Col>
      {{/each}}
    </Row>
  </Checkbox.Group>
</Form.Item>
