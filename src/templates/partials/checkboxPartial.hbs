<Form.Item 
  {{#if fieldProps.label}}
  label="{{ fieldProps.label }}"
  {{/if}}
  {{#if fieldProps.name}}
  name="{{ fieldProps.name }}"
  {{/if}}
  {{#if fieldProps.extra}}
  extra="{{ fieldProps.extra }}"
  {{/if}}
  {{#if fieldProps.rules}}
  rules={[ 
    {{#each fieldProps.rules }}
    {{{this}}},
    {{/each}} 
  ]}
  {{/if}}
  valuePropName="checked" 
  {{#if (not fieldProps.label)}}
  wrapperCol=\{{ 
    sm: { span: 24 }, 
    md: { span: 18, offset: 6 },
    lg: { span: 20, offset: 4 },
  }}
  {{/if}}
  >
  <Checkbox>{{ fieldProps.text }}</Checkbox>
</Form.Item>
